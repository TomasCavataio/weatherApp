{
    "$schema": "./node_modules/@angular/service-worker/config/schema.json",
    "index": "/index.html",
    "dataGroups": [
      {
        "name": "api-performance",
        "urls": ["/api"],
        "cacheConfig": {
          "strategy": "performance",
          "maxSize": 100,
          "maxAge": "30d"
        }
      }
    ],
    "assetGroups": [
      {
        "name": "app",
        "installMode": "prefetch",
        "resources": {
          "files": ["/favicon.ico", "/index.html", "/*.css", "/*.js"]
        }
      },
      {
        "name": "assets",
        "installMode": "lazy",
        "updateMode": "prefetch",
        "resources": {
          "files": ["/assets/**"]
        }
      }
    ]
  }
  